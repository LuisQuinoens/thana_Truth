{
    "name": "Verify",
    "version":"1.0",
    "description": "Verifies a website sources",
    "manifest_version":2,
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["contents.js"]
        }
    ],
    
    "browser_action":{
        "default_popup": "popup.html"
    }
}
